                    <li class="nav-item">
                        <a href="/Citizenships" class="nav-link bg-success text-white">Гражданства</a>
                    </li>
                    <li class="nav-item">
                        <a href="/AvailableDocuments" class="nav-link bg-primary text-white">Список документов</a>
                    </li>
                    <li class="nav-item">
                        <a href="/Settings" class="nav-link bg-dark text-white">Настройки</a>
                    </li>

                </ul>
            </nav>

        </div>
    </div>

    <div class="container body-content">
        @RenderBody()
    </div>
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>

